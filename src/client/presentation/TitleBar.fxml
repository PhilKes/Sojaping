<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import org.kordamp.ikonli.javafx.FontIcon?>

<HBox fx:id="titleBar" alignment="CENTER_RIGHT" prefWidth="600.0" stylesheets="@resources/main.css"
      xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Button fx:id="btnMinimize" contentDisplay="CENTER" mnemonicParsing="false" styleClass="orange">
            <graphic>
                <FontIcon iconLiteral="fa-window-minimize" iconSize="18" strokeType="OUTSIDE" strokeWidth="0.0"
                          textAlignment="CENTER"/>
            </graphic>
            <HBox.margin>
                <Insets/>
            </HBox.margin>
        </Button>
        <Button fx:id="btnClose" contentDisplay="CENTER" mnemonicParsing="false" styleClass="red">
            <graphic>
                <FontIcon iconLiteral="fa-window-close" iconSize="18" strokeType="OUTSIDE" strokeWidth="0.0"/>
            </graphic>
        </Button>
    </children>
</HBox>
